{
	"name": "wireplus-extension",
	"displayName": "wireplus Extension",
	"description": "A language server example",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"version": "1.0.2",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/taichimaeda/wireplus-extension.git"
	},
	"publisher": "taichimaeda",
	"categories": [],
	"keywords": [
		"multi-root ready"
	],
	"engines": {
		"vscode": "^1.75.0"
	},
	"activationEvents": [
		"onLanguage:go"
	],
	"main": "./client/out/extension",
	"contributes": {
		"commands": [
			{
				"command": "wireplus.showGraph",
				"title": "Show graph in webview (wireplus)",
				"category": "wireplus"
			},
			{
				"command": "exampleExtension.showDetail",
				"title": "Show details in webview (wireplus)",
				"category": "wireplus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"lint": "eslint ./client/src --ext .ts,.tsx",
		"postinstall": "cd client && npm install && cd ..",
		"test": "sh ./scripts/e2e.sh"
	},
	"devDependencies": {
		"@types/mocha": "^9.1.0",
		"@types/node": "^16.18.34",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.35.0",
		"mocha": "^9.2.1",
		"typescript": "^5.2.2"
	}
}